    "gemini":
    {
        "server":
        {
            "host":"127.0.0.1",
            "port":1965
        },
        "response":
        {
          "default":
          {
            "code":20,
            "meta":"text/gemini; charset=utf-8;"
          }
        }
    },
    "memcached":
    {
        "server":
        {
            "host":"127.0.0.1",
            "port":11211,
            "timeout":3600,
            "namespace":"geminiapp"
        }
    },
    "sqlite":
    {
        "server":
        {
            "name":"127.0.0.1/data.db",
            "user":"",
            "password":"",
            "timeout":600
        }
    },
    "kevachat":
    {
        "about":
        [
            "KevaChat is distributed chat platform for open, uncensored and privacy respectable communication with permanent data storage in blockchain.",
            "",
            "=> /room/NfqYRoWFZAydVyS4GjDa6o31CDSpGXVyXE Official room",
            "=> https://github.com/kevachat/geminiapp Geminiapp source",
            "=> https://github.com/kevachat/webapp Webapp source"
        ],
        "room":
        {
            "key":
            {
                "regex":"/^[\\w\\s\\._-]{2,64}$/ui"
            }
        },
        "post":
        {
            "key":
            {
                "regex":"/^([\\d]{1,64})@([A-z0-9-_]{1,64})$/"
            },
            "value":
            {
                "regex":"/.{1,3072}/ui"
            },
            "pool":
            {
                "account":"pool",
                "timeout":3600,
                "confirmations":1
            },
            "cost":
            {
                "kva":1
            }
        },
        "user":
        {
            "name":
            {
                "regex":"/^[0-9A-z-]{2,16}$/ui"
            }
        }
    }
}